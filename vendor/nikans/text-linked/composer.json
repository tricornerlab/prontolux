{
    "name": "nikans/text-linked",
    "description": "Text field with link (i.e. to this resource) on Index.",
    "keywords": [
        "laravel",
        "nova"
    ],
    "license": "MIT",
    "require": {
        "php": ">=7.1.0"
    },
    "autoload": {
        "psr-4": {
            "Nikans\\TextLinked\\": "src/"
        }
    },
    "extra": {
        "laravel": {
            "providers": [
                "Nikans\\TextLinked\\FieldServiceProvider"
            ]
        }
    },
    "config": {
        "sort-packages": true
    },
    "minimum-stability": "dev",
    "prefer-stable": true
}
